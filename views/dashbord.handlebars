            <div class="panel-heading">
              <h3 class="panel-title">Create a Potluck</h3>
            </div>
            <div class="panel-body">
              <form id="createPotluck" >
                <div class="form-group">
                  <label for="potluckName">Potluck Name</label>
                  <input class="form-control" id="potluckName" type="text" required>
                </div>
                <div class="form-group">
                  <label for="theme">Theme</label>
                  <input class="form-control" id="theme" type="text" required>
                </div>
                <div class="form-group">
                  <label for="potluckTime">Potluck Time </label>
                  <input class="form-control" id="potluckTime" type="time" required>
                </div>
                <button class="btn btn-primary" id="submitPotluck" type="submit">Submit</button>
              </form>
            </div>
          </div>
        </div>

      </div>
